package com.dinesh;

import java.util.Scanner;

public class Main {

    public static void main( String[] args) {
    Scanner in = new Scanner(System.in);
    int sum=in.nextInt();
    for(int i=0;i<=sum;i++){
      for(int j=0;j<=sum;j++){
         if(i==0||j=0||i==sum||j==sum){
            System.out.print("* ");
          }
          else
            System.out.print("  ");
       }
        System.out.println();
    }
  }
}
